<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="cn.studentfragment.www.AnswerTeacherTopicActivity">

    <cn.mywidget.www.www.BackActionbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/activity_answer_teacher_topic_textview"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:gravity="center"/>

    <ListView
        android:id="@+id/activity_answer_teacher_topic_listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></ListView>
</LinearLayout>
